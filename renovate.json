{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:js-lib",
    "github>algolia/renovate-config-algolia"
  ],
  "baseBranches": [
    "chore/renovateBaseBranch"
  ],
  "packageRules": [
    {
      "packagePatterns": [
        "^@types"
      ],
      "depTypeList": [
        "devDependencies"
      ],
      "rangeStrategy": "replace"
    }
  ],
  "prHourlyLimit": 20
}
